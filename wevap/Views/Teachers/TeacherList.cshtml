@model IEnumerable<wevap.Models.Teacher>

@{
    ViewBag.Title = "TeacherList";
}
<br />
<h2>TeacherList</h2>
<hr />
<br />
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NameTeacher)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NameTeacher)
        </td>
        <td>
            @Html.ActionLink("+Notas", "ScoreTeacher", new { id=item.DNI }) |
            @Html.ActionLink("+Materias", "SubjectTeacher", new { id=item.DNI }) 
        </td>
    </tr>
}

</table>
